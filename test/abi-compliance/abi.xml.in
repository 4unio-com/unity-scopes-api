<version>
@UNITY_SCOPES_MAJOR@.@UNITY_SCOPES_MINOR@.@UNITY_SCOPES_MICRO@
</version>

<headers>
@CMAKE_SOURCE_DIR@/include
@CMAKE_BINARY_DIR@/src
@CMAKE_BINARY_DIR@/include/unity
</headers>

<!--
We skip the internal include directories because we have separate tests
that already ensure that no public header includes an internal one.
-->
<skip_include_paths>
@CMAKE_BINARY_DIR@/include/unity/scopes/internal
@CMAKE_BINARY_DIR@/include/unity/scopes/internal/lttng
@CMAKE_BINARY_DIR@/include/unity/scopes/internal/smartscopes
@CMAKE_BINARY_DIR@/include/unity/scopes/internal/zmq_middleware
@CMAKE_BINARY_DIR@/include/unity/scopes/qt/internal
@CMAKE_BINARY_DIR@/include/unity/scopes/utility/internal
@CMAKE_SOURCE_DIR@/src/scopes/internal
@CMAKE_SOURCE_DIR@/src/scopes/internal/lttng
@CMAKE_SOURCE_DIR@/src/scopes/internal/smartscopes
@CMAKE_SOURCE_DIR@/src/scopes/qt/internal
@CMAKE_SOURCE_DIR@/src/scopes/utility/internal
</skip_include_paths>

<libs>
@CMAKE_BINARY_DIR@/libunity-scopes.so
</libs>

<gcc_options>
-std=c++11 -fPIC -D_ENABLE_QT_EXPERIMENTAL_ -DQT_NO_KEYWORDS
</gcc_options>

<skip_namespaces>
unity::scopes::internal
unity::scopes::qt::internal
unity::scopes::utility::internal
</skip_namespaces>

<skip_headers>
@CMAKE_BINARY_DIR@/include/unity/Makefile
@CMAKE_BINARY_DIR@/include/unity/scopes/internal/lttng/Makefile
@CMAKE_BINARY_DIR@/include/unity/scopes/internal/Makefile
@CMAKE_BINARY_DIR@/include/unity/scopes/internal/smartscopes/Makefile
@CMAKE_BINARY_DIR@/include/unity/scopes/internal/zmq_middleware/Makefile
@CMAKE_BINARY_DIR@/include/unity/scopes/Makefile
@CMAKE_BINARY_DIR@/include/unity/scopes/qt/internal/Makefile
@CMAKE_BINARY_DIR@/include/unity/scopes/qt/Makefile
@CMAKE_BINARY_DIR@/include/unity/scopes/testing/Makefile
@CMAKE_BINARY_DIR@/include/unity/scopes/utility/internal/Makefile
@CMAKE_BINARY_DIR@/include/unity/scopes/utility/Makefile
@CMAKE_SOURCE_DIR@/include/unity/scopes/internal/lttng/SimpleTracepoint.h
@CMAKE_SOURCE_DIR@/include/unity/scopes/internal/lttng/UnityScopes_tp.h
</skip_headers>

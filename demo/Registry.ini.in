[Registry]
Middleware = Zmq
Zmq.Endpoint = ipc:///tmp/Registry
Zmq.EndpointDir = /tmp
Zmq.ConfigFile = @CMAKE_CURRENT_BINARY_DIR@/Zmq.ini
Scope.InstallDir = @CMAKE_CURRENT_BINARY_DIR@/scopes
OEM.InstallDir = /custom/usr/lib/@CMAKE_LIBRARY_ARCHITECTURE@/scopes
Scoperunner.Path = @CMAKE_BINARY_DIR@/scoperunner/scoperunner
SS.Registry.Identity = SSRegistry
SS.Registry.Endpoint = ipc:///tmp/SSRegistry

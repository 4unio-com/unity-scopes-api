# Global configuration settings for the scopes run time.
[Runtime]

# Unless components specifically override this in their own configuration, this is the middleware that will
# be used by default. (Legal values are Ice or REST.)
Default.Middleware = Ice

# The name of the config file for the middleware factory.
Factory.Configfile = Factory.ini

# Identity of the registry. This is provided here, in Runtime.ini rather than Registry.ini because
# the registry is our bootstrap object. We need one fixed identity to get off the ground.
Registry.Identity = Registry

# The name of the registry config file.
Registry.Configfile = Registry.ini
